  <artifactId>eif-framework</artifactId>
  <name>all in one</name>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-javadoc</id>
            <phase>deploy</phase>
            <goals>
              <goal>jar</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <show>public</show>
          <charset>UTF-8</charset>
          <encoding>UTF-8</encoding>
          <docencoding>UTF-8</docencoding>
          <excludePackageNames />
          <links>
            <link>http://docs.oracle.com/javase/6/docs/api</link>
          </links>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>${maven-compiler-plugin.version}</version>
        <configuration>
          <source>${java.version}</source>
          <target>${java.version}</target>
          <encoding>utf8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>1.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <createSourcesJar>true</createSourcesJar>
              <promoteTransitiveDependencies>true</promoteTransitiveDependencies>
              <artifactSet>
                <includes>
                  <include>com.eif.framework:eif-framework-common-utils</include>
                  <include>com.eif.framework:eif-framework-config</include>
                  <include>com.eif.framework:eif-framework-cache</include>
                  <include>com.eif.framework:eif-framework-mq</include>
                  <include>com.eif.framework:eif-framework-kafka</include>
                  <include>com.eif.framework:eif-framework-concurrent</include>
                  <include>com.eif.framework:eif-framework-kv</include>
                  <include>com.eif.framework:eif-framework-fs</include>
                  <include>com.eif.framework:eif-framework-fs-g</include>
                  <include>com.eif.framework:eif-framework-job</include>
                  <include>com.eif.framework:eif-framework-data</include>
                  <include>com.eif.framework:eif-framework-log</include>
                  <include>com.eif.framework:eif-framework-mybatis-pagination</include>
                  <include>com.eif.framework:eif-framework-rpc</include>
                  <include>com.eif.framework:eif-framework-tcc-api</include>
                  <include>com.eif.framework:eif-framework-tcc-core</include>
                  <include>com.eif.framework:eif-framework-tcc-spring</include>
                </includes>
              </artifactSet>
              <transformers />
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
